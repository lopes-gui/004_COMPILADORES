argumento:INTEIRO
fatorial:INTEIRO

:ALGORITMO
// Calcula o fatorial de um nÃºmero inteiro
argumento = Integer.parseInt(System.console.readLine())
fatorial = argumento
if(argumento == 0) {
	fatorial = 1
}
while(argumento > 1) {
	fatorial = fatorial * (argumento - 1)
	argumento --
}
System.out.println("Fatorial = " + fatorial)